{
  "name": "personalcms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 3030",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev -p 3030",
    "generate:types": "payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3030"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.583.0",
    "@aws-sdk/lib-storage": "^3.583.0",
    "@nouance/payload-better-fields-plugin": "^1.4.1",
    "@payloadcms/db-mongodb": "3.0.0-beta.28",
    "@payloadcms/next": "3.0.0-beta.28",
    "@payloadcms/plugin-cloud-storage": "3.0.0-beta.28",
    "@payloadcms/richtext-lexical": "3.0.0-beta.28",
    "@payloadcms/storage-s3": "3.0.0-beta.28",
    "@payloadcms/translations": "3.0.0-beta.28",
    "@payloadcms/ui": "3.0.0-beta.28",
    "aws-crt": "^1.21.2",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "lexical": "0.15.0",
    "next": "14.3.0-canary.7",
    "payload": "3.0.0-beta.28",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "0.32.6",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "dotenv": "^16.4.5",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
